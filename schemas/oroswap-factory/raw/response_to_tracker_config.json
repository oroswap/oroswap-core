{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TrackerConfig",
  "type": "object",
  "required": [
    "code_id",
    "token_factory_addr"
  ],
  "properties": {
    "code_id": {
      "description": "Tracking contract code id",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "token_factory_addr": {
      "description": "Token factory module address",
      "type": "string"
    }
  },
  "additionalProperties": false
}
